{
    "manifest_version": 2,
    "name": "LazyTranslator",
    "version": "0.11.8",
    "description": "translate & lookup dictionary easily of selected texts",
    "permissions": [
        "activeTab",
        "storage",
        "unlimitedStorage"
    ],
    "icons": {
        "48": "icon.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./js/translate.js"
            ],
            "css": [
                "./css/translate.css"
            ]
        }
    ],
    "options_ui": {
      "page": "options.html"
    }
}